{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "special": {
      "require": "require"
    }
  },
  "diagnostics": {
    "globals": [
      "vim"
    ]
  },
  "workspace": {
    "library": [
      "${3rd}/luv/library"
    ],
    "checkThirdParty": false
  },
  "telemetry": {
    "enable": false
  }
}
